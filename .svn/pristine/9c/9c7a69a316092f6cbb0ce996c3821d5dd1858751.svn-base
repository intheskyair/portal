package com.zqkj.bsm.downmodel.bean;

import com.zqkj.bsm.downmodel.annotation.ModelTable;

@ModelTable(value="t_car_info",describe="车辆信息")
public class Car {
	@ModelTable(value="car_no",describe="车牌号(必须为数字大写字母组成的5位字符串)")
	private String car_no;
	@ModelTable(value="car_attribute",describe="车辆属性（例：行政用车）")
	private String car_attribute;
	@ModelTable(value="car_category",describe="车辆类别（例：小轿车）")
	private String car_category;
	@ModelTable(value="car_seat_num",describe="座位数(大于0的整数)")
	private String car_seat_num;
	@ModelTable(value="car_brand",describe="汽车品牌")
	private String car_brand;
	@ModelTable(value="car_type",describe="车辆型号")
	private String car_type;
	@ModelTable(value="car_status",describe="车辆状态(例:1)(1=空闲;2=已出;3=维修)")
	private String car_status;
	
	@ModelTable(value="org_name",describe="所属部门")
	private String department_id;
}
