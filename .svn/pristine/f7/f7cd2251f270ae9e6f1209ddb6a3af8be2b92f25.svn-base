package com.zqkj.bsm.system.action.util;

public class ExamineListExport {
	public static String getColName(String key){
		String value = "";
		if ("USERID".equals(key)) {
			value = "申请人ID";
		}else if("COMPANY".equals(key)){
			value = "公司名称";
		}else if("NICKNAME".equals(key)){
			value = "申请人";
		}else if("CREATETIME".equals(key)){
			value = "提交时间";
		}else if ("STATUS".equals(key)) {
			value = "审核状态";
		}else if ("RESIDENT".equals(key)) {
			value = "注册地";
		}else if ("ADDRESS".equals(key)) {
			value = "公司地址";
		}else if ("ORGANIZATIONNO".equals(key)) {
			value = "组织机构代码";
		}else if ("BUSINESSLICENSENO".equals(key)) {
			value = "营业执照号";
		}else if ("TAXREGISTRATIONNO".equals(key)) {
			value = "税务登记号";
		}else if ("LEGALPERSON".equals(key)) {
			value = "法人";
		}else if ("PHONE".equals(key)) {
			value = "公司电话";
		}else if ("CONTACTS".equals(key)) {
			value = "联系人";
		}else if ("WEBSITE".equals(key)) {
			value = "公司官网";
		}else if ("REMARK".equals(key)) {
			value = "认证说明";
		}else if ("APPROVED".equals(key)) {
			value = "审核人";
		}else if ("APPROVETIME".equals(key)) {
			value = "审批时间";
		}
		return value;
	}
	
}
