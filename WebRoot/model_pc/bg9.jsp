<script type="text/javascript">
 $(function(){
	 $("input[type='radio']").attr("disabled",true);
	 var args = window.location.search.substr(1);
		args = args.replace(/&/g,",");
		var d_arr = args.split(",");
		var obj = new Object();
		for ( i = 0; i < d_arr.length ; i++)
		{
			var d_arr_str = d_arr[i].split("=");
			obj[d_arr_str[0]] = d_arr_str[1];
		}
		
		$("input[type='checkbox']").attr("disabled",true);
		$.ajax({
			type: "get",
			url: "mzpc!getKHPF.action",
			cache: false,
			data: {mainid:obj.mainid,radom:obj.radom},
			success: function(data){
				console.log(1);
				if($.trim(data).length != 0){
					data = JSON.parse(data);
					$("#msg").val(data.MSG);
					var data1 = JSON.parse(data.PARA_VALUE);
					var one = data1.one;
					var two = data1.two;
					var three = data1.three;
					if(three != undefined){
						$("#qt").val(decodeURI(three));	
					}
					for(var i = 0; i < one.length  ; i++){
						$("#row"+i+"_"+one[i]+"").attr("checked","checked");
					}
					
					$("input[name='tm_5']:checkbox").each(function(){ 
						for(var i = 0;i < two.length ; i ++){
							if($(this).val() == two[i]){
								$(this).attr('checked', 'checked');
							}
						}
					})

				}
			}
		});

	});
</script>
